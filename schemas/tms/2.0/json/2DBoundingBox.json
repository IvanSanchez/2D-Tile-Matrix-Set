{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"description": "Minimum bounding rectangle surrounding a 2D resource in the CRS indicated elsewere",
	"type": "object",
	"required": ["lowerLeft", "upperRight"],
	"properties": {
		"crs" : {
			"$ref": "crs.json"
		},
		"lowerLeft" : {
			"$ref": "2DPoint.json"
		},
		"upperRight" : {
			"$ref": "2DPoint.json"
		}
	}
}
