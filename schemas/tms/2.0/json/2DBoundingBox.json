{
      "$schema": "http://json-schema.org/draft-07/schema#",
	"description": "Minimum bounding rectangle surrounding a 2D resource in the CRS indicated elsewere",
	"type": "object",
	"required": ["lowerLeft", "upperRight"],
	"properties": {
		"crs" : {
			"$ref": "crs.json"
		},
		"axisLabels" : {
			"type": "array",
			"minItems": 2,
			"maxItems": 2,
			"items": {
				"type": "string"
			}
		},
		"lowerLeft" : {
			"$ref": "2DPoint.json"
		},
		"upperRight" : {
			"$ref": "2DPoint.json"
		}
	}
}
