{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "title": "Keyword",
  "oneOf": [
	{
		"description": "Simplification of the object into a string if the other properties are not present", 
		"type": "string"
	},
	{
		"type": "object",
		"required": 
		[
			"keyword"
		],
		"properties":
		{
			"keyword":
			{
				"type": "string",
				"examples": ["land cover"]
			},
			"code":
			{
				"type": "string",
				"examples": ["4612"]
			},
			"codeSpace":
			{
				"type": "string",
				"examples": ["https://www.eionet.europa.eu/gemet/en/concept/"]
			}
		}
	}
  ]
}
